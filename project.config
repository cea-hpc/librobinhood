[access]
	inheritFrom = All-Projects
[submit]
	action = rebase always
[access "refs/*"]
	owner = group cea-hpc
	owner = group user/Sebastien Gougeaud (SebaGougeaud)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Sebastien Gougeaud (SebaGougeaud)
	label-Verified = -1..+1 group user/Sebastien Gougeaud (SebaGougeaud)
	push = group user/Sebastien Gougeaud (SebaGougeaud)
	read = group user/Sebastien Gougeaud (SebaGougeaud)
	rebase = group user/Sebastien Gougeaud (SebaGougeaud)
	removeReviewer = group user/Sebastien Gougeaud (SebaGougeaud)
	submit = group user/Sebastien Gougeaud (SebaGougeaud)
[access "refs/heads/*"]
	create = group cea-hpc
	create = group user/Sebastien Gougeaud (SebaGougeaud)
	push = +force group cea-hpc
	push = +force group user/Sebastien Gougeaud (SebaGougeaud)
	pushMerge = group user/Sebastien Gougeaud (SebaGougeaud)
	read = group user/Sebastien Gougeaud (SebaGougeaud)
[access "refs/tags/*"]
	push = +force group cea-hpc
	push = group user/Sebastien Gougeaud (SebaGougeaud)
	push = +force group user/Sebastien Gougeaud (SebaGougeaud)
	createSignedTag = group cea-hpc
	createTag = group cea-hpc
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[plugin "zuul-results-summary"]
	enabled = false
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
[label "Code-Review"]
	function = MaxWithBlock
	defaultValue = 0
	value = -2 This shall not be merged
	value = -1 I would prefer this is not merged as is
	value = 0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	copyCondition = change-kind:TRIVIAL_REBASE OR change-kind:NO_CHANGE
